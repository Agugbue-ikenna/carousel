{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 107",
    "not chrome <= 107",
    "not edge <= 107",
    "not opera <= 91",
    "not samsung <= 18",
    "not and_ff <= 106",
    "not firefox <= 106",
    "not ios_saf <= 16.1",
    "not safari <= 16.1"
  ],
  "hints": {
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "highest-available-document-mode": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  }
}